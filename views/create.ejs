    <script type="text/javascript">
    // request permission on page load
    document.addEventListener('DOMContentLoaded', function () {
      if (!Notification) {
        alert('Desktop notifications not available in your browser. Try Chromium.'); 
        return;
      }

      if (Notification.permission !== "granted")
        Notification.requestPermission();
    });

    function notifyMe() {
      if (Notification.permission !== "granted")
        Notification.requestPermission();
      else {
        var notification = new Notification('Super Casual Meeting that Everyone has to show up before starting', {
          icon: 'http://primspinning.ru/images/forumicons/birthday.png',
          body: "This Optional Meeting is set to start in 30 minutes. X V",
        });

        notification.onclick = function () {
          console.log("ffs");
        };

      }

    }

    function giveSummary() {
        var noti = new Notification('Join Meeting', {
            icon: 'img/toddo.jpg',
            body: "The All Hands Meeting has Begun! Click to Join Now."
        });
    }
    </script>
