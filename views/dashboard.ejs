    <script type="text/javascript">
    // request permission on page load
    document.addEventListener('DOMContentLoaded', function () {
      if (!Notification) {
        alert('Desktop notifications not available in your browser. Try Chromium.'); 
        return;
      }

      if (Notification.permission !== "granted")
        Notification.requestPermission();
    });

    function notifyMe() {
      if (Notification.permission !== "granted")
        Notification.requestPermission();
      else {
        var notification = new Notification('Rational Software All Hands Meeting', {
          icon: 'img/toddo.jpg',
          body: "Hey minion, you're demoing in an hour. Mind giving me a quick summary?",
        });

        notification.onclick = function () {
          window.open("http://stackoverflow.com/a/13328397/1269037");      
        };

      }

    }

    function giveSummary() {
        var noti = new Notification('Provide Summary for Demo', {
            icon: 'img/toddo.jpg',
            body: "Todd Baseden requested a summary for an upcoming meeting coming up in 5 minutes. Fill out now."
        });
    }

    </script>
